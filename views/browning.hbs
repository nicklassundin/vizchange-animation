<!-- extends default--><!DOCTYPE html>
<html>
    {{> head}}
	<body>
    {{> script this}}
    {{#each acts}}
        <div class="content" id={{this.id}}>
            {{#each this.scens}}
                <div id={{this.id}} class="segment">
                    {{> segment parameters=this}}
                </div>
            {{/each}}
            {{#each this.resources}}
                <div>
                    {{> resources parameters=this }}
                </div>
            {{/each}}
        </div>

        {{#if next.redirect includeZero=true}}

            <div class="segment">
            <div class="fotnote">
                <div class="menu">
                    {{#each next.redirect}}
                        <a class="button" href="{{this.href}}">{{this.text}}</a>
                    {{/each}}
                </div>
            </div>
            </div>
        {{/if}}
    {{/each}}

    {{> margin}}
	</body>
</html>
